{
  "name": "webpack-sdk",
  "version": "1.0.0",
  "description": "webpack sdk.",
  "main": "index.js",
  "author": "liu",
  "license": "MIT",
  "scripts": {
    "compile": "rimraf ./dist-build-ts && tsc",
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "lint": "eslint 'src/**/*.js' || true",
    "prettify": "prettier --write 'src/**/*.js'",
    "build:sdk": "rimraf distjs/* && npm run build:sdk:dev && npm run build:sdk:prod",
    "build:sdk:dev": "webpack --config ./build/webpack.config.js",
    "build:sdk:prod": "cross-env NODE_ENV=production webpack --config ./build/webpack.prod.js",
    "build:ts:sdk": "rimraf distTs/* && npm run build:ts:sdk:dev && npm run build:ts:sdk:prod && npm run build:ts:sdk:all",
    "build:ts:sdk:dev": "webpack --config ./build-ts/webpack.config.js",
    "build:ts:sdk:prod": "cross-env NODE_ENV=production webpack --config ./build-ts/webpack.prod.js",
    "build:ts:sdk:all": "cross-env NODE_ENV=production webpack --config ./build-ts/webpack.prod.all.js "
 },
  "keywords": [
    "webpack",
    "webpack 5",
    "webpack sdk"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "mediasoup-client": "^3.6.69",
    "socket.io-client": "^2.4.0",
    "webrtc-adapter": "7.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-function-sent": "^7.8.3",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.14.5",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "@webpack-cli/serve": "^1.6.1",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "es-expand": "^1.0.16",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.5.0",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^2.7.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "package-tls": "^1.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.3",
    "postcss-url": "^8.0.0",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "4.2.2",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-remove-debug": "^0.1.0"
  }
}